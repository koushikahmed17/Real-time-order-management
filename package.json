{
  "name": "order-management",
  "version": "1.0.0",
  "description": "Express.js Order Management API with TypeScript, PostgreSQL, and Prisma",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/prisma/schema.prisma",
    "prisma:seed": "tsx src/prisma/seed.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "postgresql",
    "zod"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "socket.io": "^4.7.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "prisma": "^5.19.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  }
}

